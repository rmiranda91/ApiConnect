@{
    ViewBag.Title = "Ver bodega";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using CentroMedicoQuirurgico.Models.Entity.Admin;
@model Tuple<ResponseAdminWareHouseList, RequestAdminWareHouse, ResponseAdminWareHouseDetail, ResponseAdminWareHouse>


<div class="row">
    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
        <h2>Vista bodegas</h2>
        <hr />
    </div>
</div>
<div class="row">
    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
        <div class="form-group">
            @Html.LabelFor(model => model.Item2.id)
            @Html.TextBoxFor(model => model.Item2.id, new { @class = "form-control", @readonly = "true" })
        </div>
        <div class="form-group">
            @Html.LabelFor(model => model.Item2.idCompany)
            @Html.DropDownListFor(model => model.Item2.idCompany, Model.Item1.lstCompany, new { @class = "form-control", @readonly = "true" })
        </div>
        <div class="form-group">
            @Html.LabelFor(model => model.Item2.responsable)
            @Html.TextBoxFor(model => model.Item2.responsable, new { @class = "form-control", @readonly = "true" })
        </div>
        <div class="form-group">
            @Html.LabelFor(model => model.Item2.description)
            @Html.TextBoxFor(model => model.Item2.description, new { @class = "form-control", @readonly = "true" })
        </div>
        <div class="form-group">
            @Html.LabelFor(model => model.Item2.stateRecord)
            @Html.DropDownListFor(model => model.Item2.stateRecord, Model.Item2.getLstState(), new { @class = "form-control", @readonly = "true" })
        </div>
        <div class="modal-footer">
            <a href="/Admin/WareHouse" class="btn btn-outline-danger">Regresar</a>
        </div>
    </div>
</div>

