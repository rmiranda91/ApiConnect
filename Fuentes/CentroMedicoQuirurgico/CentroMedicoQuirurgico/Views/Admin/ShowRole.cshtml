@{
    ViewBag.Title = "Ver perfil";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using CentroMedicoQuirurgico.Models.Entity.Admin;
@model RequestAdminRole


<div class="row">
    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
        <h2>Vista Perfil</h2>
        <hr />
    </div>
</div>
<div class="row">
    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
        <div class="form-group">
            @Html.LabelFor(model => model.name)
            @Html.TextBoxFor(model => model.name, new { @class = "form-control", @readonly = "true" })
        </div>
        <div class="form-group">
            @Html.LabelFor(model => model.detail)
            @Html.TextBoxFor(model => model.detail, new { @class = "form-control", @readonly = "true" })
        </div>
        <div class="form-group">
            @Html.LabelFor(model => model.stateRecord)
            @Html.DropDownListFor(model => model.stateRecord, Model.getLstState(), new { @class = "form-control", @readonly = "true" })
        </div>
        <div class="modal-footer">
            <a href="/Admin/Role" class="btn btn-outline-danger">Regresar</a>
        </div>
    </div>
</div>

